<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Sample Category"
            id="com.advantest.sampleView">
      </category>
      <view
            name="Sample View"
            inject="true"
            icon="icons/sample.png"
            category="com.advantest.sampleView"
            class="com.advantest.sampleview.views.SampleView"
            id="com.advantest.sampleview.views.SampleView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="com.advantest.sampleview.views.SampleView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.advantest.sampleview.command.RunHandler"
            id="com.advantest.sampleView.command.run"
            name="runHello">
      </command>
      <command
            defaultHandler="com.advantest.sampleview.command.DebugHandler"
            id="com.advantest.sampleView.command.debug"
            name="debugFuck">
      </command>
      <command
            id="com.advantest.sampleView.command.contextCommand"
            name="name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="runMenu"
               label="runHello"
               tooltip="hello every day">
            <command
                  commandId="com.advantest.sampleView.command.run"
                  label="runHello1"
                  style="push"
                  tooltip="run your application too">
            </command>
            <command
                  commandId="com.advantest.sampleView.command.run"
                  label="runHello2"
                  style="push"
                  tooltip="run hello world2">
            </command>
            <separator
                  name="com.advantest.sampleView.command.separator"
                  visible="true">
            </separator>
            <menu
                  id="subMenu"
                  label="subMenu">
               <command
                     commandId="com.advantest.sampleView.command.run"
                     icon="icons/sample.png"
                     label="subRun1"
                     style="push">
               </command>
               <command
                     commandId="com.advantest.sampleView.command.run"
                     label="sunRun2"
                     style="push">
               </command>
            </menu>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="com.advantest.sampleView.toolbar1">
            <command
                  commandId="com.advantest.sampleView.command.debug"
                  icon="icons/sample@2x.png"
                  label="debughello"
                  style="push">
            </command>
         </toolbar>
         <toolbar
               id="com.advantest.sampleView.toolbar2">
            <command
                  commandId="com.advantest.sampleView.command.run"
                  icon="icons/sample.png"
                  label="runfuck"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:com.advantest.sampleview.views.SampleView">
         <separator
               name="com.advantest.sampleView.separator1"
               visible="true">
         </separator>
         <command
               commandId="com.advantest.sampleView.command.run"
               icon="icons/sample.png"
               label="NoName"
               style="push">
         </command>
         <command
               commandId="com.advantest.sampleView.command.run"
               icon="icons/sample.png"
               style="push">
         </command>
         <separator
               name="com.advantest.sampleView.separator2"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.advantest.sampleview.views.SampleView?before=testSeparator">
         <command
               commandId="com.advantest.sampleView.command.contextCommand"
               label="one item selected"
               style="push">
         </command>
         <command
               commandId="com.advantest.sampleView.command.contextCommand"
               label="two item seletecd"
               style="push">
         </command>
         <separator
               name="com.advantest.sampleView.separator3"
               visible="true">
         </separator>
         <menu
               label="label">
            <command
                  commandId="com.advantest.sampleView.command.contextCommand"
                  label="aaa"
                  style="push">
            </command>
            <command
                  commandId="com.advantest.sampleView.command.contextCommand"
                  label="bbb"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.advantest.sampleview.views.SampleView">
         <command
               commandId="com.advantest.sampleView.command.contextCommand"
               label="one item selected"
               style="push">
         </command>
         <command
               commandId="com.advantest.sampleView.command.contextCommand"
               label="two item seletecd"
               style="push">
         </command>
         <separator
               name="com.advantest.sampleView.separator4"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:com.advantest.sampleview.views.SampleView">
         <separator
               name="com.advantest.sampleView.separator10"
               visible="true">
         </separator>
         <command
               commandId="com.advantest.sampleView.command.run"
               icon="icons/sample.png"
               label="NoName"
               style="push">
         </command>
         <command
               commandId="com.advantest.sampleView.command.run"
               icon="icons/sample.png"
               style="push">
         </command>
         <separator
               name="com.advantest.sampleView.separator11"
               visible="true">
         </separator>
         <menu
               label="label">
            <command
                  commandId="com.advantest.sampleView.command.contextCommand"
                  style="push">
            </command>
            <command
                  commandId="com.advantest.sampleView.command.contextCommand"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.advantest.sampleview.command.ShowObjectItem"
            commandId="com.advantest.sampleView.command.contextCommand">
      </handler>
   </extension>

</plugin>
